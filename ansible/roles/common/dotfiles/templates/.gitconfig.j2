[core]
    excludesfile = ~/.gitignore_global
[grep]
    extendRegexp = true
    lineNumber = true
[alias]
    g = grep --break --heading --line-number
[user]
    name = Mark Larah
    email = {{ email }}
[diff]
    noprefix = true
